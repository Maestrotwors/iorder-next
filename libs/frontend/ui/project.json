{
  "name": "frontend-ui",
  "$schema": "../../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/frontend/ui/src",
  "prefix": "io",
  "tags": ["frontend-layer:ui"],
  "projectType": "library",
  "targets": {
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/frontend/ui/jest.config.ts"
      }
    },
    "build": {
      "executor": "@nx/js:tsc",
      "options": {
        "main": "./src/index.ts",
        "additionalEntryPoints": ["./src/lib/components/button/src/index.ts", "./src/lib/components/checkbox/src/index.ts"],
        "generateExportsField": true
      }
    }
  }
}
